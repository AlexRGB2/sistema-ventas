generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Usuario {
  cveusuario    Int       @id @default(autoincrement()) @db.SmallInt
  nombre        String    @db.VarChar(250)
  apellidos     String    @db.VarChar(600)
  username      String?   @db.VarChar(150)
  password      String?   @db.VarChar(800)
  fecharegistro DateTime? @default(now()) @db.Date
  email         String
  rol           String[]

  @@map("tbl_usuario")
}
